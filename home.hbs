{{!< landingpage}}


<nav class="df w-100 border-box pv2-l ph3 main-navigation">
  <div class="flex justify-center v-mid w-75-ns w-90">
    <a class="sans-serif dim spaced-text ttu near-black f6-l f7 link ml3 mr3 sm-ml4 sm-mr4 mt3 mb3 dib fw7 link pt2 pb2" href="/blog">Blog</a>
    <a class="sans-serif dim spaced-text ttu near-black f6-l f7 link ml3 mr3 sm-ml4 sm-mr4 mt3 mb3 dib fw7 link pt2 pb2" target="_blank" href="https://github.com/bausk">GitHub</a>
  </div>
</nav>

<section class="bg-near-black sans-serif white ph4-l pa3">
  <header class="vh-90 mauto max-width-extra-large ph4-l ph0-ns pv4">
    <div class="dtc">
      <div class="justify-center flex flex-row-l flex-column items-center">
        <img id="avatar" src="assets/images/screenshot-desktop.jpg" class="pointer mw6-l mw5 pr4-l br-100 animate-glitch" onmouseover="hover(this);" onmouseout="unhover(this);">
        <div class="ph4-l ph3">
          <h2 class="f2-l f3 lh-title mt0" id="hey">Ahoy mateys,<a class="anchorjs-link " aria-label="Anchor" data-anchorjs-icon="" href="#hey" style="font: 1em/1 anchorjs-icons; padding-left: 0.375em;"></a></h2>
          <h1 class="f2-l f3 lh-title fade-in fade-in-delay-short">
            I’m <span class="cursor-rock tracked-light gold">Kiko Beats</span>, <span class="cursor-unicorn" id="job-name" data-typist="Web Engineer,Product Engineer,Landing Page Engineer,Unicorn Engineer,Full Stack Engineer,Inspect Element Engineer,Memory Leak Engineer,NaN Engineer,Asynchronous Engineer">a
              millennial doing stuff on internet</span> working at <a target="blank" href="https://www.research.ibm.com" class="cursor-work link link-underline white link-job">IBM</a>.
          </h1>
          <div class="cursor-age cf fade-in fade-in-delay-short">
            <dl class="fl fn-l w-50 dib-l w-auto-l lh-title mr5-l grow">
              <dd class="f6 fw3 ml0">Age</dd>
              <dd class="f3 ml0">26</dd>
            </dl>
            <dl class="cursor-location fl fn-l w-50 dib-l w-auto-l lh-title mr5-l grow">
              <dd class="f6 fw3 ml0">From</dd>
              <dd class="f3 ml0">Murcia, Spain</dd>
            </dl>
            <dl class="cursor-code fl fn-l w-50 dib-l w-auto-l lh-title mr5-l grow">
              <dd class="f6 fw3 ml0">Language</dd>
              <dd class="f3 ml0">JavaScript</dd>
            </dl>
            <dl class="cursor-windsurf fl fn-l w-50 dib-l w-auto-l lh-title mr5-l grow">
              <dd class="f6 fw3 ml0">Favorite Sport</dd>
              <dd class="f3 ml0">Windsurfing</dd>
            </dl>
          </div>
          <div class="pt4 fade-in fade-in-delay-large">
            <a target="blank" href="https://twitter.com/Kikobeats" title="Twitter" class="f6 dib pr2 gray dim">Twitter</a>
            <a target="blank" href="https://github.com/Kikobeats" title="GitHub" class="f6 dib pr2 gray dim" data-visited="true">GitHub</a>
            <a target="blank" href="https://www.linkedin.com/in/kikobeats" title="LinkedIn" class="f6 dib pr2 gray dim">LinkedIn</a>
          </div>
        </div>
      </div>
    </div>
  </header>
</section>


<div class="post-feed">
{{#foreach posts}}

    {{!-- The tag below includes the markup for each post - partials/post-card.hbs --}}
    {{> "post-card"}}

{{/foreach}}
</div>

